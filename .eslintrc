{
  "extends": ["airbnb-base", "plugin:prettier/recommended"],
  "env": {
    "jest": true,
    "node": true,
    "es6": true
  },
  "rules": {
    "no-prototype-builtins": 0,
    "no-underscore-dangle": 0,
    "class-methods-use-this": 0,
    "no-param-reassign": 0,
    "import/no-cycle": 0,
    "iterators/generators": 0,
    "import/prefer-default-export": 0
  },
  "parser": "babel-eslint",
  "parserOptions": {
    "sourceType": "module",
    "ecmaVersion": 10,
    "esversion": 6
  }
}
